import java.util.*;
class GraphMatrix{int v; int mat[][]; GraphMatrix(int v){this.v=v; mat=new int[v][v];} void addEdge(int u,int w){mat[u][w]=1; mat[w][u]=1;} void display(){for(int i=0;i<v;i++){for(int j=0;j<v;j++) System.out.print(mat[i][j]+" "); System.out.println();}} public static void main(String[] args){Scanner sc=new Scanner(System.in); int n=sc.nextInt(),e=sc.nextInt(); GraphMatrix g=new GraphMatrix(n); for(int i=0;i<e;i++) g.addEdge(sc.nextInt(),sc.nextInt()); g.display();}}
