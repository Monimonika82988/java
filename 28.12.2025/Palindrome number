import java.util.*;
class PalindromeNumber {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt(),temp=n,rev=0;
        while(temp>0){
            rev=rev*10+temp%10;
            temp/=10;
        }
        System.out.println(n==rev);
    }
}
