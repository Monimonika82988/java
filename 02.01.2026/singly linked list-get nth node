import java.util.*;
class Node{
    int data; Node next;
    Node(int d){data=d; next=null;}
}
class SinglyLinkedList{
    Node head;
    void insertEnd(int d){Node n=new Node(d); if(head==null) head=n; else{Node t=head; while(t.next!=null) t=t.next; t.next=n;}}
    int getNth(int n){Node t=head; for(int i=0;i<n && t!=null;i++) t=t.next; return t!=null?t.data:-1;}
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        SinglyLinkedList l=new SinglyLinkedList();
        int n=sc.nextInt();
        for(int i=0;i<n;i++) l.insertEnd(sc.nextInt());
        int k=sc.nextInt();
        System.out.println(l.getNth(k));
    }
}
