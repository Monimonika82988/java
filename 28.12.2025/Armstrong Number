import java.util.*;
class ArmstrongNumber {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt(),sum=0,temp=n;
        while(temp>0){
            int d=temp%10;
            sum+=d*d*d;
            temp/=10;
        }
        System.out.println(sum==n);
    }
}
