import java.util.*;
class MaxHeap{int heap[], size; MaxHeap(int n){heap=new int[n]; size=0;} void insert(int d){int i=size++; heap[i]=d; while(i>0 && heap[i]>heap[(i-1)/2]){int t=heap[i]; heap[i]=heap[(i-1)/2]; heap[(i-1)/2]=t; i=(i-1)/2;}} void display(){for(int i=0;i<size;i++) System.out.print(heap[i]+" ");} public static void main(String[] args){Scanner sc=new Scanner(System.in); MaxHeap h=new MaxHeap(100); int n=sc.nextInt(); for(int i=0;i<n;i++) h.insert(sc.nextInt()); h.display();}}
