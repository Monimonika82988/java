import java.util.*;
class MergeArrays{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),m=sc.nextInt();
        int a[]=new int[n];
        int b[]=new int[m];
        for(int i=0;i<n;i++)a[i]=sc.nextInt();
        for(int i=0;i<m;i++)b[i]=sc.nextInt();
        int c[]=new int[n+m];
        int k=0;
        for(int x:a)c[k++]=x;
        for(int x:b)c[k++]=x;
        for(int x:c)System.out.print(x+" ");
    }
}
